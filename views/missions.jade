extends layout

block content
  p
    a(href="/") Go back
  table(style="width:100%;")
    tr
      td(colspan="2")
        b Possible Missions (#{possible.length})
    each m in possible
      tr
        td
          input.finish(type="checkbox", value="#{m}")
        td= m
    tr
      td(colspan="2")
        b Impossible Missions (#{impossible.length})
    each m in impossible
      tr
        td
          input.finish(type="checkbox", value="#{m}")
        td= m
    tr
      td(colspan="2")
        b Finished Missions (#{done.length})
    each m in done
      tr
        td
         input.undo(type="checkbox", value="#{m}")
        td= m


